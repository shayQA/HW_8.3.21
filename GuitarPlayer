package com.company;

public class GuitarPlayer extends Player {


    @Override
    public void play() {
        System.out.println("Playing hotel california.");
    }

    @Override
    public void compose() {
        System.out.println("composing guitar melody.");
    }

    public void  rockAndRoll()
    {
        System.out.println("let the show begin.");
    }
}
